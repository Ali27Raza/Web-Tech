<div class="products">
  <% products.forEach(function(product) { %>
  <div class="product-item" id="product-<%= product.id %>">
    <a href="#">
      <img src="<%= product.image %>" alt="<%= product.title %>" />
    </a>
    <h2><%= product.title %></h2>
    <p class="price">$<%= product.price.toFixed(2) %></p>
    <a href="#" class="shop-now">SHOP NOW</a>
  </div>
  <% }); %>
</div>
